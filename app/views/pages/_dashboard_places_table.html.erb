<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Created</th>
      <th><!-- actions --></th>
    </tr>
    <tbody>
      <% places.each do |place| %>
        <tr>
          <td><%= place.name %></td>
          <td><%= time_ago_in_words(place.created_at) %></td>
          <td>
            <%= link_to 'edit', edit_place_path(place)%> |
            <%= link_to 'delete', place_path(place), method: :delete %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </thead>
</table>
